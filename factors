#!/usr/bin/python3
"""Module that factorize as many numbers as possible. """
from sys import argv


def factorize(value):
    """"A simple descomposition of an integer > 1"""
    a = 2

    if val < 2:
        return
    print()
    print(val, "<- value-bef")
    while val % a:
        a += 1
    print("{:.0f}={:.0f}*{:.0f}".format(val, val / a, a))
    print(val, "<- value-aft")
    print()

if len(argv) != 2:
    exit()

try:
    with open(argv[1]) as file:
        line = file.readline()

        while line != "":
            value = int(line.split('\n')[0])
            factorize(val)
            line = file.readline()
except:
    pass
